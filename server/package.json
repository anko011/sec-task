    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@swc/core": "^1.10.14",
    "@types/express": "^5.0.0",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@vitest/coverage-v8": "^3.0.5",
